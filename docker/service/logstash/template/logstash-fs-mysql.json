{
  "index_patterns": ["logstash-fs-mysql-*"],
  "template": {
    "settings": {
      "number_of_shards": 1,
      "number_of_replicas": 0
    },
    "mappings": {
      "properties": {
        "message": {
          "type": "text"
        },
        "@version": {
          "type": "long"
        },
        "@timestamp": {
          "type": "date"
        },
        "log_offset": {
          "type": "long"
        },
        "log_filepath": {
          "type": "keyword"
        },
        "log_timestamp": {
          "type": "date"
        },
        "log_hostname": {
          "type": "keyword"
        },
        "user": {
          "type": "keyword"
        },
        "hostname": {
          "type": "keyword"
        },
        "ip": {
          "type": "keyword"
        },
        "id": {
          "type": "integer"
        },
        "query_time": {
          "type": "double"
        },
        "lock_time": {
          "type": "double"
        },
        "rows_sent": {
          "type": "long"
        },
        "rows_examined": {
          "type": "long"
        },
        "timestamp": {
          "type": "date"
        },
        "sql": {
          "type": "keyword",
          "ignore_above": 65535
        }
      }
    }
  }
}
